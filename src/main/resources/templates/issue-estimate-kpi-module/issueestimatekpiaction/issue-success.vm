<html>
<head>
    <title>用户</title>
    <meta name="decorator" content="atl.admin">
    <script type="text/javascript">
    </script>
</head>
<body>

<section role="dialog" id="loading-dialog" class="aui-layer aui-dialog2 aui-dialog2-medium" aria-hidden="true">
    <!-- Dialog header -->
    <header class="aui-dialog2-header">
        <!-- The dialog's title -->
        <h2 class="aui-dialog2-header-main">正在加载</h2>
        <!-- Actions to render on the right of the header -->
        <!-- Close icon -->
        #*<a class="aui-dialog2-header-close">
            <span class="aui-icon aui-icon-small aui-iconfont-close-dialog">Close</span>
        </a>*#
    </header>
    <!-- Main dialog content -->
    <div class="aui-dialog2-content" style="text-align: center;">
            <div id="search-area-example" class="custom-card-style">
                <p>Loading...</p>
                <span class="aui-icon aui-icon-wait">loading</span>
            </div>
    </div>
    <!-- Dialog footer -->
    #*<footer class="aui-dialog2-footer">
        <!-- Actions to render on the right of the footer -->
        *##*<div class="aui-dialog2-footer-actions">
            <button id="dialog-submit-button" class="aui-button aui-button-primary">OK</button>
            <button id="dialog-next-button" class="aui-button">Next</button>
            <button id="dialog-close-button" class="aui-button aui-button-link">Close</button>
        </div>*##*
        <!-- Hint text is rendered on the left of the footer -->
        <div class="aui-dialog2-footer-hint">this is a hint</div>
    </footer>*#
</section>

<div class="aui-group">
    <div class="aui-item">
        <div class="field-group">
            <div class="user-browser__user-search">
                <input class="text full-width-field" style="width: 50%;height:25px" id="user-filter-userSearchFilter" placeholder="jql语法" maxlength="255" name="userSearchFilter" type="text">
                <button id="searchButton" class="aui-item aui-button aui-button-subtle search-button" type="button" original-title="搜索问题">
                    <span class="aui-icon aui-icon-small aui-iconfont-search">搜索</span>
                </button>
            </div>
        </div>
    </div>
</div>
<div class="aui-group">
    <div id="error-context" class="aui-item">

    </div>
</div>
<div class="aui-group">
    <div class="aui-group aui-group-split count-pagination">
        <div class="results-count aui-item">
            共 <span class="results-count-total" id="total"></span>个。
        </div>
        <div class="aui-item">
            <input id="startAt" type="hidden" value="$startAt">
            <input id="currentPage" type="hidden" value="$currentPage">
            <div id="pageNumber" class="pagination">

            </div>
        </div>
    </div>
</div>
<div id="issueTable" class="aui-group">
    <table width="100%" class="aui" id="single_groupby_report_table">
        <thead>
        <tr>
            <th id="basic-number">#</th>
            <th id="basic-fname">用户名</th>
            <th id="basic-lname">关键字</th>
            <th id="basic-username">项目名称</th>
            <th id="basic-username">分数</th>
        </tr>
        </thead>
        <tbody>
            <tr>
                <td width="5%" headers="basic-fname" id="show_hide_button">
                    <span class="aui-icon aui-icon-small aui-iconfont-arrows-down" status="down">Insert meaningful text here for accessibility</span>
                </td>
                <td width="25%" headers="basic-fname">Matt</td>
                <td width="25%" headers="basic-lname">Bond</td>
                <td width="25%" headers="basic-username">mbond</td>
                <td width="20%" headers="basic-number">1</td>
            </tr>
            <tr class="Matt" style="display:none">
                <td width="5%"></td>
                <td width="25%"></td>
                <td width="25%" headers="basic-lname">Chaldecott</td>
                <td width="25%" headers="basic-username">rchaldecott</td>
                <td width="20%" headers="basic-number">2</td>
            </tr>
            <tr class="Matt" style="display:none">
                <td width="5%"></td>
                <td width="25%"></td>
                <td width="25%" headers="basic-lname">Tapia</td>
                <td width="25%" headers="basic-username">htapia</td>
                <td width="20%" headers="basic-number">3</td>
            </tr>
        </tbody>
    </table>
</div>
</body>
</html>

